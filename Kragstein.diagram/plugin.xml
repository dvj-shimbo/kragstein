      <modelingAssistantProvider class="project.diagram.providers.assistants.ProjectModelingAssistantProviderOfProjectEditPart">
         <Priority name="Lowest"/>
         <object class="project.diagram.edit.parts.ProjectEditPart" id="Project_1000"/>
         <context elements="Project_1000"/>
      </modelingAssistantProvider>
            <modelingAssistantProvider class="project.diagram.providers.assistants.ProjectModelingAssistantProviderOfPackageEditPart">
         <Priority name="Lowest"/>
         <object class="project.diagram.edit.parts.PackageEditPart" id="Package_2001"/>
         <context elements="Package_2001"/>
      </modelingAssistantProvider>
      
   </extension>


   <extension point="org.eclipse.gmf.runtime.common.ui.services.iconProviders" id="icon-provider">
      <?gmfgen generated="true"?>
      <IconProvider class="project.diagram.providers.ProjectIconProvider">
         <Priority name="Low"/>
      </IconProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.parserProviders" id="parser-provider">
      <?gmfgen generated="true"?>
      <ParserProvider class="project.diagram.providers.ProjectParserProvider">
         <Priority name="Lowest"/>
      </ParserProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes" id="element-types">
      <?gmfgen generated="true"?>
      <metamodel nsURI="http:///project.ecore">
         <metamodelType
               id="Kragstein.diagram.Project_1000"
               name="%metatype.name.Project_1000"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="Project"
               edithelper="project.diagram.edit.helpers.ProjectEditHelper">
            <param name="semanticHint" value="1000"/>
         </metamodelType>
      </metamodel>
      <metamodel nsURI="http:///project.ecore">
         <metamodelType
               id="Kragstein.diagram.Package_2001"
               name="%metatype.name.Package_2001"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="Package"
               edithelper="project.diagram.edit.helpers.PackageEditHelper">
            <param name="semanticHint" value="2001"/>
         </metamodelType>
      </metamodel>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings" id="element-types-bindings">
      <?gmfgen generated="true"?>
      <clientContext id="Kragstein.diagram.TypeContext">
         <enablement>
            <test
               property="org.eclipse.gmf.runtime.emf.core.editingDomain"
               value="Kragstein.diagram.EditingDomain"/>
         </enablement>
      </clientContext> 
      <binding context="Kragstein.diagram.TypeContext">
         <elementType ref="Kragstein.diagram.Project_1000"/>
         <elementType ref="Kragstein.diagram.Package_2001"/>
         <advice ref="org.eclipse.gmf.runtime.diagram.core.advice.notationDepdendents"/>
      </binding>
   </extension>
	   <extension point="org.eclipse.core.expressions.propertyTesters" id="navigator-proptest.isURIEditorInput">
      <?gmfgen generated="true"?>
       <propertyTester
           id="Kragstein.diagram.URIEditorInputPropertyTester"
           type="org.eclipse.emf.common.ui.URIEditorInput"
           namespace="Kragstein.diagram"
           properties="isURIEditorInput"
           class="project.diagram.part.ProjectUriEditorInputTester">
       </propertyTester>
   </extension>

	   <extension point="org.eclipse.ui.navigator.viewer" id="navigator-viewbinding">
      <?gmfgen generated="true"?>
      <viewerContentBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension pattern="Kragstein.diagram.resourceContent"/>
			<contentExtension pattern="Kragstein.diagram.domainResourceContent"/>
            <contentExtension pattern="Kragstein.diagram.navigatorLinkHelper"/>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <actionExtension pattern="Kragstein.diagram.navigatorActionProvider"/>
         </includes>
      </viewerActionBinding>
   </extension>

   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="Kragstein.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="project.diagram.navigator.ProjectNavigatorContentProvider" 
            labelProvider="project.diagram.navigator.ProjectNavigatorLabelProvider"
            icon="icons/obj16/ProjectDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="project_diagram"/>
               </and>
               <instanceof value="project.diagram.navigator.ProjectAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="project.diagram.navigator.ProjectAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="Kragstein.diagram.navigatorSorter" 
               class="project.diagram.navigator.ProjectNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="project_diagram"/>
                  </and>
                  <instanceof value="project.diagram.navigator.ProjectAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="Kragstein.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="project.diagram.navigator.ProjectDomainNavigatorContentProvider" 
            labelProvider="project.diagram.navigator.ProjectDomainNavigatorLabelProvider"
            icon="icons/obj16/ProjectDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="project"/>
               </and>
               <instanceof value="project.diagram.navigator.ProjectDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="project.diagram.navigator.ProjectDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="Kragstein.diagram.navigatorActionProvider"
            class="project.diagram.navigator.ProjectNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="project.diagram.navigator.ProjectAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
	   <extension point="org.eclipse.ui.navigator.linkHelper" id="navigator-linkhelper">
      <?gmfgen generated="true"?>
      <linkHelper
            id="Kragstein.diagram.navigatorLinkHelper"
            class="project.diagram.navigator.ProjectNavigatorLinkHelper">
         <editorInputEnablement>
            <and>
               <instanceof value="org.eclipse.emf.common.ui.URIEditorInput"/>
               <test property="Kragstein.diagram.isURIEditorInput"/>
            </and>
         </editorInputEnablement>
         <selectionEnablement>
            <instanceof value="project.diagram.navigator.ProjectAbstractNavigatorItem"/>
         </selectionEnablement>
      </linkHelper>
   </extension>
   <extension point="org.eclipse.ui.commands" id="update-cmd">
