         name="%editorName"
         icon="icons/obj16/ProjectDiagramFile.gif"
         extensions="project_diagram"
         default="true"
         class="project.diagram.part.ProjectDiagramEditor"
         matchingStrategy="project.diagram.part.ProjectMatchingStrategy"
         contributorClass="project.diagram.part.ProjectDiagramActionBarContributor">
      </editor>
   </extension>
   
   <extension point="org.eclipse.ui.contexts" id="ui-context">
      <?gmfgen generated="true"?>
