  	     name="%newWizardName"
  	     icon="icons/obj16/ProjectDiagramFile.gif"
  	     category="org.eclipse.ui.Examples"
  	     class="project.diagram.part.ProjectCreationWizard"
  	     id="project.diagram.part.ProjectCreationWizardID">
